{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "Application.Name": {
      "value": "#{Api.Name}#"
    },
    "Application.Environment": {
      "value": "#{App.Environment.Code}#"
    },
    "Application.Version": {
      "value": "#{Artifact.Version}#"
    },
    "Release.Name": {
      "value": "#{Release.ReleaseName}#"
    },
    "Release.RequestedFor": {
      "value": "#{Release.Deployment.RequestedFor}#"
    },
    "Release.SourceCodeBranch": {
      "value": "#{Release.SourceCodeBranch}#"
    },
    "Release.TriggerType": {
      "value": "#{Release.Reason}#"
    },
    "Release.Url": {
      "value": "#{Release.ReleaseWebUrl}#"
    },
    "Keyvault.SkuName": {
      "value": "#{Keyvault.SkuName}#"
    },
    "Keyvault.ObjectId": {
      "value": "#{Infra.DevOps.ObjectId}#"
    },
    "Keyvault.TenantId": {
      "value": "#{Infra.DevOps.TenantId}#"
    }
  }
}
